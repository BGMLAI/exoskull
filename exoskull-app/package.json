{
  "name": "exoskull",
  "version": "0.1.0",
  "private": true,
  "description": "Adaptive Life Operating System - Self-optimizing multi-tenant AI assistant",
  "scripts": {
    "dev": "next dev",
    "build": "NODE_OPTIONS='--max-old-space-size=4096' next build",
    "start": "next start",
    "lint": "next lint",
    "lint:a11y": "eslint --config .eslintrc.accessibility.js --ext .ts,.tsx,.js,.jsx .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "voice-ws": "npx tsx server/voice-ws.ts",
    "voice-ws:dev": "npx tsx watch server/voice-ws.ts",
    "test:smoke": "echo \"Run: npm run test:routes (requires dev server on localhost:3000)\"",
    "test:routes": "npx tsx scripts/test-all-routes.ts",
    "test:routes:prod": "npx tsx scripts/test-all-routes.ts --base-url https://exoskull.xyz",
    "test:a11y": "node accessibility-test-suite.js",
    "test:a11y:prod": "BASE_URL=https://exoskull.xyz node accessibility-test-suite.js",
    "prepare": "cd .. && husky exoskull-app/.husky || true",
    "supabase:gen-types": "supabase gen types typescript --project-id uvupnwvkzreikurymncs > lib/database.types.ts",
    "migrate-tyrolka": "npx tsx scripts/migrate-to-tyrolka.ts"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write"
    ],
    "*.{json,md,css}": [
      "prettier --write"
    ]
  },
